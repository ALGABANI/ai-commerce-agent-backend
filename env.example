# Copy to .env and fill securely. On Render, set them as Environment Variables.
PROJECT_NAME=AI Commerce Agent
API_V1_STR=/api

# Neon/Render Postgres URL (psycopg v3) with SSL
DATABASE_URL=postgresql+psycopg://USER:PASSWORD@HOST/DBNAME?sslmode=require

# JWT secret (use a strong random string)
JWT_SECRET_KEY=change-me

# Optional: comma-separated origins that can call this API from browser
CORS_ORIGINS=https://gabanilogistics.com,https://www.gabanilogistics.com,https://ai-commerce-agent-backend.onrender.com
